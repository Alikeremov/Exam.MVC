@model EmployeeCreateVm

<div class="col-md-6 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Default form</h4>
            <p class="card-description">
                Basic form layout
            </p>
            <form class="forms-sample" method="post" enctype="multipart/form-data">
                <div class="form-group">
                    <label asp-for="Photo" >Photo</label>
                    <input asp-for="Photo" class="form-control" placeholder="Username">
                    <span asp-validation-for="Photo"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Name" >Name</label>
                    <input asp-for="Name" class="form-control" placeholder="Name">
                    <span asp-validation-for="Name"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Surname">Surname</label>
                    <input asp-for="Surname" class="form-control" placeholder="Surname">
                    <span asp-validation-for="Surname"></span>
                </div>
                <div class="form-group">
                    <label asp-for="About">About</label>
                    <input asp-for="About" class="form-control" placeholder="About">
                    <span asp-validation-for="About"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Fblink">Fblink</label>
                    <input asp-for="Fblink" class="form-control" placeholder="Fblink">
                    <span asp-validation-for="Fblink"></span>
                </div>
                <div class="form-group">
                    <label asp-for="InstagramLink">InstagramLink</label>
                    <input asp-for="InstagramLink" class="form-control" placeholder="InstagramLink">
                    <span asp-validation-for="InstagramLink"></span>
                </div>
                <div class="form-group">
                    <label asp-for="TwLink">TwLink</label>
                    <input asp-for="TwLink" class="form-control" placeholder="TwLink">
                    <span asp-validation-for="TwLink"></span>
                </div>
                <div class="form-group">
                    <label asp-for="GoogleLink">GoogleLink</label>
                    <input asp-for="GoogleLink" class="form-control" placeholder="GoogleLink">
                    <span asp-validation-for="GoogleLink"></span>
                </div>
                <button type="submit" class="btn btn-primary me-2">Submit</button>
                <a asp-action="index" asp-controller="Employee" class="btn btn-light">Cancel</a>
            </form>
        </div>
    </div>
</div>